        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Si tu le souhaites, tu peux bouger doucement ton pouce pour explorer cette sensation."
          }
        },
        "it" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Se vuoi, puoi muovere delicatamente il pollice, esplorando questa sensazione."
          }
