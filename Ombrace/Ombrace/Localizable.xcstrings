          "stringUnit" : {
            "state" : "translated",
            "value" : "Come ti fa sentire?"
          }
        }
      }
    },
    "How would you like to be called?" : {
      "comment" : "The title of the view where the user can insert their name inside a text field",
      "localizations" : {
        "it" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Come vorresti essere chiamato?"
          }
